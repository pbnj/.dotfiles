[
    {
        "key": "ctrl+shift+`",
        "command": "workbench.action.terminal.newWithCwd",
        "args": {
            "cwd": "${fileDirname}"
        }
    },
    {
        "command": "-markdown.extension.editing.toggleBold",
        "key": "cmd+b",
        "when": "editorTextFocus && !editorReadonly && editorLangId == 'markdown'"
    },
    {
        "command": "-workbench.action.focusSideBar",
        "key": "cmd+0"
    },
    {
        "command": "extension.vsKubernetesExplorer.focus",
        "key": "ctrl+alt+cmd+k"
    },
    {
        "command": "dockerContainers.focus",
        "key": "ctrl+alt+cmd+d"
    },
    {
        "command": "atlascode.views.jira.customJql.focus",
        "key": "ctrl+alt+cmd+j"
    },
    {
        "command": "-workbench.action.files.newUntitledFile",
        "key": "cmd+n"
    },
    {
        "command": "editor.action.quickFix",
        "key": "ctrl+.",
        "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
        "command": "-editor.action.quickFix",
        "key": "cmd+.",
        "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
        "command": "-hediet.vscode-drawio.linkCodeWithSelectedNode",
        "key": "shift+f3"
    },
    {
        "command": "todo-tree-view.focus",
        "key": "ctrl+alt+cmd+c"
    },
    {
        "command": "-workbench.action.terminal.openNativeConsole",
        "key": "shift+cmd+c",
        "when": "!terminalFocus"
    },
    {
        "command": "workbench.action.compareEditor.nextChange",
        "key": "down",
        "when": "textCompareEditorVisible"
    },
    {
        "command": "-workbench.action.compareEditor.nextChange",
        "key": "alt+f5",
        "when": "textCompareEditorVisible"
    },
    {
        "command": "workbench.action.compareEditor.previousChange",
        "key": "up",
        "when": "textCompareEditorVisible"
    },
    {
        "command": "-workbench.action.compareEditor.previousChange",
        "key": "shift+alt+f5",
        "when": "textCompareEditorVisible"
    },
    {
        "key": "shift+cmd+t",
        "command": "-workbench.action.reopenClosedEditor"
    },
    {
        "key": "shift+cmd+t",
        "command": "workbench.action.tasks.runTask"
    },
    {
        "key": "ctrl+cmd+j",
        "command": "workbench.action.terminal.resizePaneDown",
        "when": "terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+cmd+h",
        "command": "workbench.action.terminal.resizePaneLeft",
        "when": "terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+cmd+l",
        "command": "workbench.action.terminal.resizePaneRight",
        "when": "terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+cmd+k",
        "command": "workbench.action.terminal.resizePaneUp",
        "when": "terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+alt+cmd+g",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "ctrl+shift+g",
        "command": "-workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "ctrl+alt+cmd+o",
        "command": "outline.focus"
    },
    {
        "key": "ctrl+shift+alt+cmd+k",
        "command": "editor.action.marker.prevInFiles",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+alt+cmd+p",
        "command": "projectsExplorerGit.focus"
    },
    {
        "key": "shift+alt+cmd+o",
        "command": "workbench.action.showAllSymbols"
    },
    {
        "key": "cmd+t",
        "command": "-workbench.action.showAllSymbols"
    },
    {
        "key": "ctrl+alt+cmd+t",
        "command": "workbench.view.testing.focus"
    },
    {
        "key": "shift+alt+cmd+p",
        "command": "projectManager.listProjectsNewWindow"
    },
    {
        "key": "cmd+n",
        "command": "workbench.action.files.newUntitledFile"
    },
    {
        "key": "cmd+n",
        "command": "explorer.newFile",
        "when": "explorerViewletFocus || filesExplorerFocus"
    },
    {
        "key": "cmd+0",
        "command": "workbench.action.zoomReset"
    },
    {
        "key": "cmd+numpad0",
        "command": "-workbench.action.zoomReset"
    },
    {
        "key": "ctrl+alt+`",
        "command": "workbench.action.toggleMaximizedPanel"
    }
]